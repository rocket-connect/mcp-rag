{
  "name": "mcp-rag",
  "version": "0.0.0",
  "description": "AI SDK tool orchestration without the headache",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r --parallel test",
    "test:integration": "pnpm run test:integration:client && pnpm run test:integration:neo4j",
    "test:integration:client": "pnpm --filter @mcp-rag/client test:integration",
    "test:integration:neo4j": "pnpm --filter @mcp-rag/neo4j test:integration",
    "clean": "pnpm -r exec rm -rf dist node_modules",
    "clean:dist": "pnpm -r exec rm -rf dist",
    "typecheck": "pnpm -r typecheck",
    "lint": "eslint packages --ext .ts,.tsx",
    "lint:fix": "eslint packages --ext .ts,.tsx --fix",
    "format": "prettier --write \"./**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,json,md}\"",
    "benchmark": "pnpm --filter @mcp-rag/benchmarks benchmark",
    "benchmark:ci": "pnpm --filter @mcp-rag/benchmarks benchmark:ci"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.3.3"
    }
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "mcp",
    "rag",
    "neo4j",
    "tools",
    "orchestration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rconnect-tech/mcp-rag.git"
  },
  "license": "MIT"
}
